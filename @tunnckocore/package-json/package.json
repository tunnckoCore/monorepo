{
  "scripts": {
    "type:gen": "tsc -p ./tsconfig.gentypes.json",
    "type:check": "tsc -p ./tsconfig.check.json",
    "build": "babel src -d dist/esm -x .js,.jsx,.ts,.tsx,.mjs --delete-dir-on-start",
    "postbuild": "ncc build dist/esm/index.js -o dist/cjs --minify",
    "test": "jest"
  },
  "module": "dist/esm/index.js",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "repository": "tunnckoCore/monorepo",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "beta-x"
  },
  "engines": {
    "node": "^8.10.0 || >=10.13.0"
  },
  "browserslist": [
    "node 8",
    "node 10"
  ],
  "files": [
    "dist",
    "index.js"
  ],
  "license": "Apache-2.0",
  "licenseStart": "2018",
  "version": "1.1.0",
  "name": "@tunnckocore/package-json",
