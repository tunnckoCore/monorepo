{
  "scripts": {
    "type:gen": "echo 'tsc -p ./tsconfig.gentypes.json'",
    "type:check": "echo 'tsc -p ./tsconfig.check.json'",
    "prebuild": "rm -rf dist",
    "build": "echo 'BABEL_ENV=build rolldown --pkg-dir $PWD'",
    "test": "echo 'devest test'",
    "docs": "docks --outfile .verb.md && verb"
  },
  "main": "src/",
  "typings": "dist/typings/index.d.ts",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "engines": {
    "node": "^8.10.0 || >=10.13.0"
  },
  "browserslist": [
    "node 8.10",
    "node 10.13"
  ],
  "files": [
    "src"
  ],
  "license": "Apache-2.0",
  "licenseStart": "2018",
  "version": "3.0.1-alpha.8",
  "name": "eslint-config-tunnckocore",
  "description": "Shareable ESLint config for @tunnckoCore/hq mono-repository",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^9.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-unicorn": "^9.0.0"
  },
  "keywords": [],
  "homepage": "https://github.com/tunnckoCore/hq/tree/master/packages/eslint-config-tunnckocore",
  "repository": {
    "type": "git",
    "url": "https://github.com/tunnckoCore/hq.git",
    "directory": "packages/eslint-config-tunnckocore"
  },
  "author": {
    "name": "Charlike Mike Reagent",
    "email": "mameto2011@gmail.com",
    "url": "https://tunnckocore.com"
  },
  "verb": {
    "data": {
      "depsType": "dep",
      "directory": "packages/foo-bar-baz-qux",
      "avatar": "https://avatars3.githubusercontent.com/u/5038030?v=4",
      "twitter": "tunnckoCore",
      "github": "tunnckoCore"
    },
    "readme": "../../config/readme-template.md",
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": []
  }
}
