    "@tunnckocore/git-semver-tags": "^0.3.1-alpha.5",
    "@types/dedent": "^0.7.0",
    "dedent": "^0.7.0",
    "fs-extra": "^7.0.0",
    "git-raw-commits": "^2.0.0",
    "parse-commit-message": "^3.2.3",
    "simple-git": "^1.107.0"
  },
