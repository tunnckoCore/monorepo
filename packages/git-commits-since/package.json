{
  "scripts": {
    "type:gen": "echo 'tsc -p ./tsconfig.gentypes.json'",
    "type:check": "echo 'tsc -p ./tsconfig.check.json'",
    "prebuild": "rm -rf dist",
    "build": "BABEL_ENV=build rolldown --pkg-dir $PWD",
    "test": "devest test",
    "docs": "docks --outfile .verb.md && verb"
